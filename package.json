{
  "name": "termite-api",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "@apollo/client": "^3.3.19",
    "bignumber.js": "^9.0.1",
    "egg": "^2.15.1",
    "egg-scripts": "^2.11.0",
    "egg-sequelize": "^6.0.0",
    "ethers": "^5.2.0",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.12.4",
    "mysql2": "^2.2.5",
    "node-fetch": "^2.6.1",
    "react": "^17.0.2",
    "web3": "^1.3.6"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0",
    "sequelize-cli": "^6.2.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --port 9900 --daemon --title=egg-server-termite-api",
    "start:bsc": "TERMITE_ENV=bsc egg-scripts start --port 9900 --daemon --title=egg-server-termite-api",
    "stop": "egg-scripts stop --title=egg-server-termite-api",
    "start:preview": "TERMITE_ENV=preview egg-scripts start --port 9901 --daemon --title=egg-server-preview-termite-api",
    "stop:preview": "egg-scripts stop --title=egg-server-preview-termite-api",
    "restart": "npm run stop && npm run start",
    "restart:preview": "npm run stop:preview && npm run start:preview",
    "dev": "egg-bin dev",
    "dev:preview": "TERMITE_ENV=preview egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
